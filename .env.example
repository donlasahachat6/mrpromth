# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Vanchin AI Configuration (7 Agents)
VANCHIN_BASE_URL=https://vanchin.streamlake.ai/api/gateway/v1/endpoints
VANCHIN_AGENT_AGENT1_KEY=your_vanchin_agent1_api_key
VANCHIN_AGENT_AGENT2_KEY=your_vanchin_agent2_api_key
VANCHIN_AGENT_AGENT3_KEY=your_vanchin_agent3_api_key
VANCHIN_AGENT_AGENT4_KEY=your_vanchin_agent4_api_key
VANCHIN_AGENT_AGENT5_KEY=your_vanchin_agent5_api_key
VANCHIN_AGENT_AGENT6_KEY=your_vanchin_agent6_api_key
VANCHIN_AGENT_AGENT7_KEY=your_vanchin_agent7_api_key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Rate Limiting (optional - defaults will be used if not set)
RATE_LIMIT_API_REQUESTS=60
RATE_LIMIT_API_WINDOW=60s
RATE_LIMIT_AUTH_REQUESTS=5
RATE_LIMIT_AUTH_WINDOW=60s
RATE_LIMIT_ADMIN_REQUESTS=100
RATE_LIMIT_ADMIN_WINDOW=60s
RATE_LIMIT_AI_REQUESTS=10
RATE_LIMIT_AI_WINDOW=60s

# Session Configuration
SESSION_TIMEOUT=3600

# User Limits (optional - can be overridden by admin settings)
MAX_PROMPTS_PER_USER=100
MAX_API_KEYS_PER_USER=5

# Feature Flags
ENABLE_USER_REGISTRATION=true
AGENT_MODE_ENABLED=true
MAINTENANCE_MODE=false
